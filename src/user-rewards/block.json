{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "apppresser/user-rewards",
	"version": "0.1.0",
	"title": "User Rewards",
	"category": "widgets",
	"icon": "star-filled",
	"description": "Displays a users earned rewards.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"rewardType": {
			"type": "string",
			"default": "promotional-reward"
		},
		"rewardTitle": {
			"type": "string",
			"default": "My Rewards"
		},
		"rewardMessage": {
			"type": "string",
			"default": "You have not earned any rewards."
		}
	},
	"textdomain": "user-rewards",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}